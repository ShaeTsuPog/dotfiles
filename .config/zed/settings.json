{
  "icon_theme": "Colored Zed Icons Theme Dark",
  "telemetry": {
    "diagnostics": false,
    "metrics": false,
  },
  "session": {
    "trust_all_worktrees": true,
  },
  "base_keymap": "VSCode",
  "vim_mode": true,
  "ui_font_size": 16,
  "buffer_font_size": 15,
  "buffer_font_family": "JetBrainsMono Nerd Font",
  "theme": {
    "mode": "dark",
    "light": "One Light",
    "dark": "Vague",
  },
  "tab_size": 2,
  "disable_ai": true,
  "colorize_brackets": true,
  "lsp_document_colors": "background",
  "lsp": {
    "lua-language-server": {
      "settings": {
        "Lua": {
          "diagnostics": {
            "globals": ["vim"],
          },
          "workspace": {
            "library": ["/usr/share/nvim/runtime"],
            "checkThirdParty": false,
          },
        },
      },
    },
  },
}
